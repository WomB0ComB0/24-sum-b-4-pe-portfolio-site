{% extends "../layout.jinja2" %}

{% block main %}
<section id="contact">
    <h2>Contact Me</h2>
          <form action="{{ url_for('contact') }}" method="POST">
              <label for="email">Email:</label>
              <input type="email" id="email" name="email" required>
              <button type="submit">Submit</button>
          </form>
</section>
{% endblock %}